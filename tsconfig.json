{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "jsxImportSource": "react",
    "paths": {
      "@public/*": ["./public/*"],
      "@svelte/*": ["./src/components/svelte/*"],
      "@vue/*":["./src/components/vue/*"]
    }
  }
}
